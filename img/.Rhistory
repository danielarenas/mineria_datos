dataset <- read.csv('P2-Movie-Ratings.csv')
dataset
library(caTools)
set.seed(123)
split <- sample.split(dataset$Budget, SplitRatio = 2/3)
training_set <- subset(dataset, split == TRUE)
test_set <- subset(dataset, split == FALSE)
regressor = lm(formula = Budget ~ YearOfRelease,
data = dataset)
summary(regressor)
y_pred = predict(regressor, newdata = test_set)
library(ggplot2)
ggplot() +
geom_point(aes(x=training_set$YearOfRelease, y=training_set$Budget,
color = 'red') +
geom_line(aes(x = training_set$YearOfRelease, y = predict(regressor, newdata = training_set)),
color = 'blue') +
ggtitle('Year of release vs Budget (Training Set)') +
xlab('YearOfRelease') +
ylab('Budget'))
